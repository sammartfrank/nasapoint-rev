{
  "name": "nasapoint-rev",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build:app": "turbo run build --filter=\"app...\"",
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint"
  },
  "devDependencies": {
    "prettier": "latest",
    "turbo": "latest"
  },
  "dependencies": {},
  "engines": {
    "node": ">=18.0.0"
  }
}
